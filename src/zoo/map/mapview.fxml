            <ImageView fitHeight="138.0" fitWidth="1080.0">
               <image>
                  <Image url="@../../res/mapviewHeader.png" />
               </image>
            </ImageView>
            <Button fx:id="btnLogin" layoutX="918.0" layoutY="110.0" mnemonicParsing="false" onAction="#pflegerLogin" prefHeight="25.0" prefWidth="148.0" text="Login">
