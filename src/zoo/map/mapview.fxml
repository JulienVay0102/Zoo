                  <Insets />
               </opaqueInsets>
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <PasswordField fx:id="txtPassword" layoutX="917.0" layoutY="75.0" promptText="Password" />
            <TextField fx:id="txtUsername" layoutX="917.0" layoutY="38.0" promptText="Username" />
            <Label fx:id="lblStatus" layoutX="918.0" layoutY="10.0" prefHeight="17.0" prefWidth="148.0" text="Status" textFill="#fcf3b0">
